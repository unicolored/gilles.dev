{
  "name": "gilles.dev",
  "version": "0.11.0",
  "type": "module",
  "scripts": {
    "i18n": "ng extract-i18n gilles.dev --format=xlf --out-file=src/locale/messages.xlf",
    "eslint": "eslint src && eslint projects",
    "check": "npx prettier --check ./src ./projects",
    "format": "npx prettier -w ./src ./projects",
    "set-env": "ts-node src/environments/set-env.ts",
    "prod": "yarn set-env && ng build ngx-services && yarn build",
    "deploy": "ngh --dir=dist/gilles.dev/browser",
    "ng": "ng",
    "dev": "ng serve --hmr false",
    "build": "ng build --configuration production --base-href \"/\"",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:gilles.dev": "node dist/gilles.dev/server/server.mjs",
    "lint": "ng lint",
    "explore": "ng build --source-map && source-map-explorer dist/app/browser/main* --no-border-checks"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.3",
    "@angular/compiler": "^20.3.3",
    "@angular/core": "^20.3.3",
    "@angular/forms": "^20.3.3",
    "@angular/platform-browser": "^20.3.3",
    "@angular/platform-browser-dynamic": "^20.3.3",
    "@angular/platform-server": "^20.3.3",
    "@angular/router": "^20.3.3",
    "@angular/ssr": "^20.3.3",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.16",
    "algoliasearch": "^5.37.0",
    "instantsearch.js": "^4.80.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "schema-dts": "^1.1.5",
    "tailwindcss": "^4.1.13",
    "three": "^0.175.0",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.2",
    "@angular/build": "20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.3",
    "@angular/localize": "^20.3.3",
    "@cloudinary/ng": "^2.1.2",
    "@cloudinary/url-gen": "^1.21.0",
    "@eslint/js": "^9.29.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.14.1",
    "@types/three": "^0.175.0",
    "angular-eslint": "20.2.0",
    "colors": "^1.4.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.29.0",
    "express": "^4.18.2",
    "globals": "^16.0.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.34.1"
  },
  "packageManager": "yarn@4.10.3"
}
